{
    "$schema": "https://turbo.build/schema.json",
    "tasks": {
        "build": {
            "dependsOn": [
                "^build"
            ],
            "outputs": [
                ".next/**",
                "!.next/cache/**",
                "dist/**"
            ],
            "env": [
                "NEXT_PUBLIC_SUPABASE_URL",
                "NEXT_PUBLIC_SUPABASE_ANON_KEY",
                "SUPABASE_SERVICE_ROLE_KEY",
                "ORGANIZATION_ID",
                "NODE_ENV"
            ]
        },
        "lint": {},
        "typecheck": {},
        "test": {
            "cache": false
        },
        "dev": {
            "cache": false,
            "persistent": true
        }
    }
}